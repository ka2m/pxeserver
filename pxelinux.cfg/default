DEFAULT vesamenu.c32
PROMPT 0
MENU TITLE Mirror Walker 

LABEL bootlocal
	MENU DEFAULT
	MENU LABEL Local Boot
	TEXT HELP
		This will exit from the network boot menu and attempt
		to boot from local media (hard disk, DVD, etc)
	ENDTEXT
	localboot 0x80

menu include pxelinux.cfg/asplinux/asplinux.conf asplinux
menu include pxelinux.cfg/centos/centos.conf centos
menu include pxelinux.cfg/debian/debian.conf debian
menu include pxelinux.cfg/debian-pure64/debian-pure64.conf debian-pure64
menu include pxelinux.cfg/fedora/fedora.conf fedora
menu include pxelinux.cfg/fedora-enchilada/fedora-enchilada.conf fedora-enchilada
menu include pxelinux.cfg/fedora-linux-development/fedora-linux-development.conf fedora-linux-development
menu include pxelinux.cfg/fedora-linux-releases/fedora-linux-releases.conf fedora-linux-releases
menu include pxelinux.cfg/mirrors/mirrors.conf mirrors
menu include pxelinux.cfg/rosa/rosa.conf rosa
menu include pxelinux.cfg/scientificlinux/scientificlinux.conf scientificlinux
menu include pxelinux.cfg/ubuntu/ubuntu.conf ubuntu
