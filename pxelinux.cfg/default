DEFAULT menu.c32
PROMPT 0
MENU TITLE Mirror Walker

LABEL bootlocal
    MENU DEFAULT
    MENU LABEL Local Boot
    TEXT HELP
        This will exit from the network boot menu and attempt
        to boot from local media (hard disk, DVD, etc)
    ENDTEXT
    localboot 0x80

LABEL FreeBSD
    KERNEL menu.c32
    APPEND pxelinux.cfg/FreeBSD/FreeBSD.conf
